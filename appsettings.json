{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MSSQLConnection": "Server={DB_SERVER};Database={DB_NAME};User ID={DB_USERNAME};Password={DB_PASSWORD};",
    "ExecutionMSSQLConnection": "Server={EXECUTE_DB_SERVER};Database={EXECUTE_DB_NAME};User ID={EXECUTE_DB_USERNAME};Password={EXECUTE_DB_PASSWORD};"
  }
}
